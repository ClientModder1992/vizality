      "@i18n": [ "./core/modules/i18n" ],
      "@builtins": [ "./core/builtins"],
      "@modules": [ "./core/modules"],
      "@patcher": [ "./core/modules/patcher" ],
      "@react": [ "./core/modules/react" ],
