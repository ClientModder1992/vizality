{
  "exclude": [ "node_modules" ],
  "compilerOptions": {
    "baseUrl": "./src/",
    "paths": {
      "@vizality/root/*": [ "./../*" ],
      "@vizality/src/*": [ "./*" ],
      "@vizality/*": [ "./core/modules/*"],
      "@vizality/api": [ "./core/api"],
      "@vizality/compilers": [ "./core/compilers" ],
      "@vizality/core": [ "./core/entities" ],
      "@vizality/entities": [ "./core/entities" ],
      "@vizality/builtins": [ "./core/builtins"],
      "@vizality/modules": [ "./core/modules"],
      "@vizality/typedefs": [ "./core/typedefs" ],
      "react": [ "./core/modules/react/React" ],
      "react-dom": [ "./core/modules/react/ReactDOM" ],
      "react-router": [ "./core/modules/react/Router" ]
    }
  }
}
