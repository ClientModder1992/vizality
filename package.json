{
  "name": "vizality",
  "author": "vizality",
  "version": "0.0.1",
  "description": "A robust @discord client mod focused on design and user customizability",
  "homepage": "https://vizality.com",
  "main": "index.js",
  "scripts": {
    "compile-sass": "node src/compile",
    "lint": "eslint --ext js --ext jsx src",
    "inject": "node src/inject inject",
    "uninject": "node src/inject uninject",
    "generate-docs": "jsdoc --configure .jsdoc.json"
  },
  "repository": "https://github.com/vizality/vizality.git",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/vizality/vizality/issues"
  },
  "_moduleAliases": {
    "@root": ".",
    "@src": "src",
    "vizality": "src/.vizality/modules",
    "@classes": "src/.vizality/modules/classes",
    "@compilers": "src/.vizality/modules/compilers",
    "@components": "src/.vizality/modules/components",
    "@constants": "src/.vizality/modules/constants",
    "@discord": "src/.vizality/modules/discord",
    "@entities": "src/.vizality/modules/entities",
    "@http": "src/.vizality/modules/http",
    "@i18n": "src/.vizality/modules/i18n",
    "@modules": "src/.vizality/modules",
    "@patcher": "src/.vizality/modules/patcher",
    "@react": "src/.vizality/modules/react",
    "@typings": "src/.vizality/typings",
    "@util": "src/.vizality/modules/util",
    "@webpack": "src/.vizality/modules/webpack"
  },
  "dependencies": {
    "codemirror": "^5.57.0",
    "image-to-base64": "^2.1.1",
    "long": "^4.0.0",
    "module-alias": "^2.2.2",
    "node-watch": "^0.6.4",
    "pluralize": "^8.0.0",
    "react": "^16.13.1",
    "react-markdown": "^4.3.1",
    "sass": "^1.26.10",
    "sucrase": "^3.15.0",
    "unzip-crx": "^0.2.0"
  },
  "devDependencies": {
    "eslint": "^7.8.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-jsdoc": "^30.4.0"
  }
}
